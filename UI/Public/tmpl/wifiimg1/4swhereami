		function nx(pa){
			var tdCountLine = 5;
			if(pa.length > 0){
				var htm = '您是不是找：';
				var i=0;
				
				//如果只有关键词的时候
				keywordLen = 3;
				if(keywordLen > pa.length){
					keywordLen = pa.length;
				}
				
				for(;i < keywordLen;i++){
					htm += "<a class='green' target='_blank' href =\"http://"+pa[i][1]+"\" onclick=\"click_log('"+pa[i][0]+"','"+pa[i][1]+"')\">"+pa[i][0]+"</a>";
				}
				
				keyDiv = document.getElementById("tips");
				keyDiv.innerHTML = htm;
				
				//clear
				htm = "";
				keywordLen = 12;
				if(keywordLen+i > pa.length){
					keywordLen = pa.length;
				}

				//add hrefs in mbar
				for(;i < keywordLen;i++){
					htm += "<a class='green' target='_blank' href =\"http://"+pa[i][1]+"\" onclick=\"click_log('"+pa[i][0]+"','"+pa[i][1]+"')\">"+pa[i][0]+"</a>";
				}
				
				mbarBD = document.getElementById("mbarbd");
				mbarBD.innerHTML = htm;
			}
	   }
