		//天气回调函数
		function callback(data){
			$("#weather  .inner .city  .name").text(data.area[0][0]);
			
			pic_num = data.weather[0].info.day[0];
			today_bg = "background:url(http://p1.qhimg.com/d/_hao360/weather/"+pic_num+".png) no-repeat;_background-image:none;_filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='http://p1.qhimg.com/d/_hao360/weather/"+pic_num+".png',sizingMethod='image')";
			$("#weather  .inner .today .pic").attr("style",today_bg);
			
			today_weather_info = "今  "+data.weather[0].info.day[1];
			today_weather_tempature = " "+data.weather[0].info.day[2]+"~"+data.weather[0].info.night[2]+"°C";
			$("#weather  .inner .today .desc").text(today_weather_info);
			$("#weather  .inner .today .temperature").text(today_weather_tempature);
			
			today_weather_extend = data.weather[0].info.day[3]+"："+data.weather[0].info.day[4];
			$("#weather  .inner .weather-extend .extend-list .selected").text(today_weather_extend);
			
			//tomorrow weather
			pic_num = data.weather[1].info.day[0];
			today_bg = "background:url(http://p1.qhimg.com/d/_hao360/weather/"+pic_num+".png) no-repeat;";
			$("#weather  .inner .tomorrow .pic").attr("style",today_bg);
			
			tomorrow_weather_info = "明  "+data.weather[1].info.day[1];
			tomorrow_weather_tempature = " "+data.weather[1].info.day[2]+"~"+data.weather[1].info.night[2]+"°C";
			$("#weather  .inner .tomorrow .desc").text(tomorrow_weather_info);
			$("#weather  .inner .tomorrow .temperature").text(tomorrow_weather_tempature);
			
			$("#weather_loading").hide();
			$("#weather").show();
		}
